{
  "name": "csharp-package-autocomplete",
  "displayName": "C# Package Autocomplete",
  "description": "Provides autocomplete for #:package, #:sdk, #:property directives in C# files",
  "version": "1.0.0",
  "publisher": "taraskovalenko",
  "author": {
    "name": "Taras Kovalenko",
    "email": "bad.kto@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TarasKovalenko/csharp-package-autocomplete"
  },
  "keywords": ["csharp", "nuget", "autocomplete", "packages", "dotnet", "sdk"],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#512BD4",
    "theme": "dark"
  },
  "contributes": {
    "languages": [{
      "id": "csharp",
      "extensions": [".cs"]
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/node": "24.x",
    "typescript": "^5.9.2",
    "@vscode/vsce": "^3.6.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}